body {
  font-family: "Trebuchet MS", Verdana, Arial, sans-serif;
  font-size: 90%;
  background: #DDD url("squares-72-blur.png") center;
}

a, a:link, a:active { color: #006900; }
a:visited { color: #555; }


.wrap {
  margin: 0 auto;
  max-width: 55em;
  background: white center;
  padding: 0 0.9em;
}

.wrap-t {
  margin: 1em auto 0 auto;
  max-width: 56.8em;
  display: block;
  height: 13px;
  padding: 0;
}

.wrap-t.lt {display: block; height: 13px; background: url("0lt.png") no-repeat left top}
.wrap-t .rt {display: block; height: 13px; background: url("0rt.png") no-repeat right top;
             padding: 0 13px;}
.wrap-t .i {display: block; height: 13px; background: white}

.wrap-b {
  margin: 0 auto 1em auto;
  max-width: 56.8em;
  display: block;
  height: 13px;
  padding: 0;
}

.wrap-b.lb {display: block; height: 13px; background: url("0lb.png") no-repeat left top}
.wrap-b .rb {display: block; height: 13px; background: url("0rb.png") no-repeat right top;
             padding: 0 13px;}
.wrap-b .i {display: block; height: 13px; background: white}

.wheader {
  background: white;
}
#header {
  height: 85px;
  position: relative;
  padding: 0;
}

#header h1, #header h2 {display: none}

.heading {
  position: relative;
  margin: 1.5ex 0 0 0 ;
  background: #DDD url("strom1.jpg") center;
  padding: 3em 2em 2em 2em;
}

.heading .text {
  font-size: 180%;
  margin: 0; padding: 0;
  font-weight: bold;
}

.heading a {
  position: absolute;
  left: 1ex; top: 1ex;
  font-size: 90%;
  background: url("p-o65.png");
  display: block;
  padding: 0.5ex;
}

.heading .text span {
  background: url("p-o65.png");
  display: block;
  padding: 0.5ex;
}

.wmenu {padding: 0 1px 1px 1px}
.wmenu2 {
  background: url("menu1rb-6.png") no-repeat right bottom;
  padding: 0 7px 5px 7px;
  text-align: right;
}
#menu {
  background: url("menu1lb-6.png") no-repeat left bottom;
  background-color: #EEE;
  border-top: #CCC 1px solid;
}

#menu a:before { content: " | ";}
#menu a {text-decoration: none}

#header .loginArea {
  position: absolute;
  top: 5px; right: 5px;
  margin: 0; padding: 0 3px;
  text-align: inherit;
}

#header .loginArea * {
  margin: 0; padding: 0;
}

#header .loginArea label {
  color: black;
}

#header .loginArea .input_small {
  width: 10em;
}

#header .loginArea input {
  padding: 1px;
}

#main {
  margin: 1.5ex 1px 0 1px;
  position: relative;
  padding: 0 15em 0 0;
  background: white;
}

#left { padding: 0 }

#right {
  position: absolute;
  top: 0;
  right: 0;
  width: 13.7em;
  padding: 0 0 0 1ex;
  border-left: solid #006900 2px;
}

.post {
  border-bottom: solid #006900 1px;
  padding: 1ex 0;
}
.post:first-child {
  padding: 0 0 1ex 0;
}

.post h1 {
  margin: 2ex 0 1ex 0;
  font-size: 180%;
}

.post h2 {
  margin: 2ex 0 1ex 0;
  font-size: 136%;
}

.post h3 {
  margin: 1ex 0 1.5ex 0;
  font-size: 100%;
}

.post .subtitle {
  margin: 1ex 0 1.5ex 0;
  font-size: 100%;
  color: #555;
  font-weight: normal;
}

h3:first-child {margin-top: 0}

.post .date {
  background: #EEE;
  border-top: solid #CCC 1px;
  padding: 0.3ex 0.5ex;
  font-size: 90%;
  margin: 0;
}

.post .status {
  margin: 1em 0 0 0;
  padding: 0.3ex 0.5ex;
  background: #EEE;
  font-size: 90%;
  border-bottom: solid #CCC 1px;
}

.read-more { display: block; margin: 1em 0 0 0}

hr { display: none }

.avatar {
  margin: 0 auto;
  width: 13em;
  padding: 0.5em 0 0.5ex 0;
}

.avatar img {width:12.9em;}
.avatar p {margin: 0; padding: 0}

.f1a {
  width: 13.7em;
  margin: 0 0 1ex 0;
}

#footer {
  margin: 0;
  padding: 1ex 1ex 0.5ex 1ex;
}

#footer p {
  text-align: right;
  margin: 0;
  padding: 0;
}

#footer-menu {
  float: left;
}

#footer img {
  display: block;
  margin: 0 0 3px 0;
}

